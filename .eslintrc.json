{
    "extends": "standard",
    "rules": {
        "indent": ["error", 4, {
            "SwitchCase": 1
        }],
        "no-mixed-operators": 0,
        "space-before-function-paren": ["error", {
            "anonymous": "always",
            "named": "ignore",
            "asyncArrow": "always"
        }],
        "no-useless-constructor": "off"
    }
}
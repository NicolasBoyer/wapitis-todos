{
  "compilerOptions": {
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "target": "es2017",
    "module": "es2015",
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es2015",
      "dom.iterable",
      "scripthost"
    ],
    "allowJs": false,
    "newLine": "LF",
    "jsx": "react",
    "jsxFactory": "JSX.createElement",
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "noImplicitAny": true,
    "alwaysStrict": true,
    "experimentalDecorators": true,
    "baseUrl": ".",
    "strictPropertyInitialization": false
  }
}